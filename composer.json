{
	"name":"nadybot/nadybot",
	"description": "The successor of Budabot",
	"type":"project",
	"keywords": ["budabot", "nadybot"],
	"homepage": "https://github.com/nadybot/nadybot/",
	"authors": [
		{
			"name": "Nadyita",
			"email": "nadyita@hodorraid.org",
			"role": "Maintainer"
		},
		{
			"name": "Tyrence",
			"role": "Original Maintainer"
		}
	],

	"require": {
		"php": ">=8.1.17",
		"ext-sqlite3": "*",
		"ext-curl": "*",
		"ext-sockets": "*",
		"ext-pdo": "*",
		"ext-pdo_sqlite": "*",
		"ext-pdo_mysql": "*",
		"ext-mbstring": "*",
		"ext-ctype": "*",
		"ext-bcmath": "*",
		"ext-json": "*",
		"ext-simplexml": "*",
		"ext-filter": "*",
		"ext-pcre": "*",
		"ext-date": "*",
		"ext-reflection": "*",
		"ext-dom": "*",
		"php-amqplib/php-amqplib": "^3.1.0",
		"swaggest/json-schema": "^0.12.31",
		"illuminate/database": "^8.64",
		"doctrine/dbal": "^3.0",
		"kelvinmo/fernet-php": "^1.0",
		"farafiri/php-parsing-tool": "^2.0",
		"monolog/monolog": "^2.3",
		"thecodingmachine/safe": "^2.0",
		"netresearch/jsonmapper": "^v4.0.0",
		"amphp/amp": "^2.6",
		"amphp/http-client": "^4.6",
		"amphp/cache": "^1.5.0",
		"amphp/file": "^2.0",
		"amphp/websocket-client": "^1.0",
		"eventsauce/object-hydrator": "^1.1.0",
		"amphp/http-tunnel": "^1.0",
		"nadylib/imex": "dev-main"
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/Nadybot/IMEX.git"
		}
	],
	"autoload": {
		"psr-4": {
			"Nadybot\\User\\Modules\\": "extras/",
			"Nadybot\\": "src/"
		}
	},
	"config": {
		"vendor-dir": "vendor",
		"allow-plugins": {
			"composer/package-versions-deprecated": true
		}
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.6",
		"composer/composer": "^2.0.0",
		"phpstan/phpstan": "^1.6",
		"vimeo/psalm": "^4.12",
		"phpstan/phpstan-deprecation-rules": "^1.0",
		"friendsofphp/php-cs-fixer": "^3.51"
	},
	"scripts": {
		"post-package-install": [
			"Nadybot\\Patcher\\Patcher::patch"
		],
		"post-package-update": [
			"Nadybot\\Patcher\\Patcher::patch"
		]
	}
}
