{
	"name":"nadybot/nadybot",
	"description": "The successor of Budabot",
	"type":"project",
	"keywords": ["budabot", "nadybot"],
	"homepage": "https://github.com/nadybot/nadybot/",
	"authors": [
		{
			"name": "Nadyita",
			"email": "nadyita@hodorraid.org",
			"role": "Maintainer"
		},
		{
			"name": "Tyrence",
			"role": "Original Maintainer"
		}
	],

	"require": {
		"php": ">=8.0.0",
		"ext-sqlite3": "*",
		"ext-curl": "*",
		"ext-sockets": "*",
		"ext-pdo": "*",
		"ext-pdo_sqlite": "*",
		"ext-pdo_mysql": "*",
		"ext-mbstring": "*",
		"ext-ctype": "*",
		"ext-bcmath": "*",
		"ext-json": "*",
		"ext-simplexml": "*",
		"ext-filter": "*",
		"ext-pcre": "*",
		"ext-date": "*",
		"ext-reflection": "*",
		"ext-dom": "*",
		"php-amqplib/php-amqplib": "^3.1.0",
		"swaggest/json-schema": "^0.12.31",
		"illuminate/database": "^8.64",
		"doctrine/dbal": "^3.0",
		"kelvinmo/fernet-php": "^1.0",
		"farafiri/php-parsing-tool": "^2.0",
		"spatie/data-transfer-object": "^3.0",
		"monolog/monolog": "^2.3",
		"thecodingmachine/safe": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"Nadybot\\": "src/",
			"Nadybot\\User\\Modules\\": "extra/"
		}
	},
	"config": {
		"vendor-dir": "vendor",
		"allow-plugins": {
			"composer/package-versions-deprecated": true
		}
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.6",
		"composer/composer": "^1.10.23",
		"phpstan/phpstan": "^1.3",
		"vimeo/psalm": "^4.12",
		"rector/rector": "^0.12.10"
	},
	"scripts": {
		"post-package-install": [
			"Nadybot\\Patcher\\Patcher::patch"
		],
		"post-package-update": [
			"Nadybot\\Patcher\\Patcher::patch"
		]
	}
}
