{
	"name":"nadybot/nadybot",
	"description": "The successor of Budabot",
	"type":"project",
	"keywords": ["budabot", "nadybot"],
	"homepage": "https://github.com/nadybot/nadybot/",
	"authors": [
		{
			"name": "Nadyita",
			"email": "nadyita@hodorraid.org",
			"role": "Maintainer"
		},
		{
			"name": "Tyrence",
			"role": "Original Maintainer"
		}
	],

	"require": {
		"php": ">=8.1.17",
		"ext-sqlite3": "*",
		"ext-curl": "*",
		"ext-sockets": "*",
		"ext-pdo": "*",
		"ext-pdo_sqlite": "*",
		"ext-pdo_mysql": "*",
		"ext-mbstring": "*",
		"ext-ctype": "*",
		"ext-bcmath": "*",
		"ext-json": "*",
		"ext-simplexml": "*",
		"ext-filter": "*",
		"ext-pcre": "*",
		"ext-date": "*",
		"ext-reflection": "*",
		"ext-dom": "*",
		"revolt/event-loop": "^1.0.6",
		"swaggest/json-schema": "^0.12.31",
		"illuminate/database": "^8.64",
		"doctrine/dbal": "^3.0",
		"kelvinmo/fernet-php": "^1.0",
		"farafiri/php-parsing-tool": "^2.0",
		"monolog/monolog": "^2.3",
		"thecodingmachine/safe": "^2.0",
		"psr/log": "^1|^2|^3",
		"amphp/amp": "^3.0.0",
		"amphp/http-client": "^5.0.1",
		"amphp/cache": "^2.0.0",
		"amphp/file": "^3.0.2",
		"amphp/websocket-client": "^2.0.0",
		"netresearch/jsonmapper": "^4.0.0",
		"eventsauce/object-hydrator": "^1.1.0",
		"amphp/http-tunnel": "^2.0.0-beta.2",
		"nadylib/imex": "dev-main",
		"nadybot/aolib": "dev-main",
		"nadylib/leaky-bucket": "dev-main",
		"amphp/pipeline": "^1.2",
		"amphp/process": "^2.0",
		"amphp/byte-stream": "^2.1",
		"amphp/parallel": "^2.2"
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/Nadybot/IMEX.git"
		},
		{
			"type": "git",
			"url": "https://github.com/Nadybot/aolib.git"
		},
		{
			"type": "git",
			"url": "https://github.com/Nadybot/leaky-bucket.git"
		}
	],
	"autoload": {
		"psr-4": {
			"Nadybot\\phpstan\\": "phpstan/src/",
			"Nadybot\\User\\Modules\\": "extras/",
			"Nadybot\\": "src/"
		}
	},
	"config": {
		"vendor-dir": "vendor",
		"allow-plugins": {
			"composer/package-versions-deprecated": true
		}
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.9.0",
		"composer/composer": "^2.0.0",
		"composer-phar/phpstan": "^1.10.59",
		"phpstan/phpstan-deprecation-rules": "^1.0",
		"composer-phar/php-cs-fixer": "^3.51.0",
		"psalm/phar": "^5.22.2",
		"thecodingmachine/phpstan-safe-rule": "^1.2"
	},
	"scripts": {
		"post-package-install": [
			"Nadybot\\Patcher\\Patcher::patch"
		],
		"post-package-update": [
			"Nadybot\\Patcher\\Patcher::patch"
		]
	}
}
